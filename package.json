{
  "name": "lm-terminal",
  "productName": "LM Terminal",
  "version": "1.0.0",
  "description": "A terminal-inspired interface for local LLMs",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "dev": "concurrently \"npm run watch\" \"npm run start-dev\"",
    "watch": "webpack --config webpack.renderer.config.js --watch",
    "start-dev": "cross-env NODE_ENV=development nodemon --watch ./src/main --watch ./src/preload --exec \"electron-forge start\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "rebuild": "electron-rebuild -f -w node-llama-cpp"
  },
  "keywords": [],
  "author": {
    "name": "Scaredofboobs",
    "email": "web3dev.work@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-fuses": "^7.8.0",
    "@electron-forge/plugin-webpack": "^7.2.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "electron": "27.1.3",
    "electron-rebuild": "^3.2.9",
    "electron-reloader": "^1.2.3",
    "html-webpack-plugin": "^5.6.3",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "node-loader": "^2.0.0",
    "nodemon": "^3.1.9",
    "path-browserify": "^1.0.1",
    "style-loader": "^3.3.4",
    "webpack": "^5.91.0",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.3",
    "@codemirror/lang-python": "^6.1.7",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.36.5",
    "@tabler/icons-react": "^2.47.0",
    "codemirror": "^6.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "fs-extra": "^11.2.0",
    "html-validator": "^6.0.1",
    "jsdom": "^26.0.0",
    "monaco-editor": "^0.52.2",
    "node-llama-cpp": "^3.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "systeminformation": "^5.25.11",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  }
}
